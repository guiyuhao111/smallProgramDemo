<view class='gray'>
  <view class='content'>
    <view class='head-img'>
      <open-data class="userImage" type="userAvatarUrl"></open-data>
      <open-data class="userName" type="userNickName"></open-data>
    </view>
    <view class='nav-wrap'>
      <view bindtap='toResumePage'>
        <image src='/image/nav1.png'>
        </image>
        <text>
            我的简历
          </text>
      </view>
      <view bindtap='toCollectPage' data-status='1'>
        <image src='/image/nav2.png'>
        </image>
        <text>
            申请记录
          </text>
      </view>
      <view bindtap='toCollectPage' data-status='2'>
        <image src='/image/nav3.png'>
        </image>
        <text>
          我的收藏
          </text>
      </view>
    </view>
  </view>
</view>